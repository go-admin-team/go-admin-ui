    "plop": "2.7.4",
    "runjs": "^4.4.2",
    "sass": "^1.35.1",
    "sass-loader": "^9.0.3",
    "script-ext-html-webpack-plugin": "2.1.4",
    "script-loader": "0.7.2",
    "serve-static": "^1.14.1",
    "svg-sprite-loader": "^5.0.0",
    "svgo": "1.3.2",
    "vue-template-compiler": "2.6.11"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
